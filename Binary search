#include<stdio.h>
int main(){
int a[50],n,key;
int low=0,high,mid;
printf("enter the size:");
scanf("%d",&n);
printf("enter sorted number:\n");
for(int i=0;i<n;i++)
    scanf("%d",&a[i]);
printf("enter number to search:");
scanf("%d",&key);
high = n - 1;
while(low <= high){
mid=(low+high)/2;
if(a[mid] == key){
    printf("found at position %d",mid+1);
    return 0;

}
else if(key < a[mid])
    high = mid-1;
else
    low = mid +1;
printf("not found");
return 0;


}}
