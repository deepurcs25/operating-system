#include<stdio.h>
int main() {
int n,a[50],key,i;
 printf("enter size of array:");
 scanf("%d",&n);

 printf("enter %d numbers:\n", n);
 for(i=0;i<n;i++){
    scanf("%d",&a[i]);

    printf("enter Numbers to search:");
    scanf("%d",&key);
    for(i=0;i<n;i++){
        if(a[i] == key){
            printf("Number found at position %d",i+1);
            return 0;

        }
    }
    printf("Number not found");
    return 0;
 }
}
