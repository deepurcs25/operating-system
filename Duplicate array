#include <stdio.h>
#include <stdio.h>

int main() {
    int arr[100], n, i, j, found = 0;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; j++) {
            if(arr[i] == arr[j]) {
                found = 1;
                break;
            }
        }
        if(found) 
break;
    }

    if(found)
        printf("Duplicates found in the array.\n");
    else
        printf("No duplicates in the array.\n");

    return 0;
}
